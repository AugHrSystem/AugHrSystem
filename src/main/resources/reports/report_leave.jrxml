	<queryString>
		<![CDATA[select l.id,l.aim, l.start_date as startDate,l.end_date as endDate, l.start_time as startTime ,l.end_time as endTime
,l.reason, m.name, l.employee_id as employee, emp.employee_code as employeeCode, m.leavetype_id as id,aim.nameEng as nameEng,aim.nameThai as nameThai,aim.dateOfBirth as dateOfBirth

from EMP_LEAVE l, EMP_EMPLOYEE emp,MAS_LEAVETYPE  m ,
(select  e.NAME_ENG as nameEng,e.NAME_THAI as nameThai,e.DATEOFBIRTH as dateOfBirth,l.ID as id from EMP_LEAVE l
join EMP_EMPLOYEE e on e.ID=l.AIM where l.AIM=e.ID) aim

where  l.employee_id = emp.id and l.employee_id = emp.id
and l.leavetype_id = m.leavetype_id and aim.id=l.id]]>
